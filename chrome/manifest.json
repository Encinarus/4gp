{
  "name": "Genconnator",
  "version": "1.0",
  "description": "Click on the button to see the event on genconplanner.com.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {},
  "content_scripts": [{
    "matches": ["https://www.gencon.com/*"],
    "js": ["content.js"]
  }],
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "webNavigation"
  ]
}
